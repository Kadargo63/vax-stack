<body>
  <div class="wrapper">
      <header class="page-header">
        <div class="app-title-container">
          <img src="../assets/logo.svg">
          <h1 class="app-title">Vax<span class="app-title italic">Stack</span></h1>
          </div>
          <div>
            <nav class"nav">
                    <a class="light-link" href="/"><i class="fas fa-list-alt" style="padding-right: 8px;"></i>All Appointments</a>
                    <a class="light-link" href="/dashboard"><i class="fas fa-notes-medical" style="padding-right: 8px;"></i>My Services</a>
                    {{#if loggedIn}}
                    <a class="btn" href="/" id="logout"><i class="fas fa-sign-out-alt" style="padding-right: 8px;"></i>Logout</a>
                    {{else}}
                    <a class="btn" href="/login"><i class="fas fa-sign-in-alt" style="padding-right:8px;"></i>Login</a>
                    {{/if}}
                </nav>          
          </div>
      </header>

    <main>
      {{{ body }}}
    </main>
  </div>

    {{#if loggedIn}}
    <script src="/javascript/logout.js"></script>
    {{/if}}

</body>

</html>